struct Builtin
{
    u8 x;
    u16 y;
};

struct BuiltinFixed
{
    u32 x[2];
};

struct BuiltinDynamic
{
    u32 x<>;
};

struct BuiltinLimited
{
    u32 x<2>;
};

struct BuiltinGreedy
{
    u32 x<...>;
};

struct BuiltinExtSizedA
{
    u8 a;
    u16 one<@a>;
    u64 two<@a>;
};

struct BuiltinExtSizedB
{
    u32 a;
    u8 one<@a>;
    u16 two<@a>;
    u32 three<@a>;
};

struct Fixcomp
{
    Builtin x;
    Builtin y;
};

struct FixcompFixed
{
    Builtin x[2];
};

struct FixcompDynamic
{
    Builtin x<>;
};

struct FixcompLimited
{
    Builtin x<2>;
};

struct FixcompGreedy
{
    Builtin x<...>;
};

struct Dyncomp
{
    BuiltinDynamic x;
};

struct DyncompDynamic
{
    BuiltinDynamic x<>;
};

struct DyncompGreedy
{
    BuiltinDynamic x<...>;
};
