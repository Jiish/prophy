         <xi:include href="MAC.xml" xpath="//interface[@name='MacTest']"/>
      </interfaces>
   </component>
   <component name="PHY">
      <xi:include href="PHY.xml" xpath="//definitions[@name='Common PHY']"/>
      <xi:include href="PHY.xml" xpath="//definitions[@name='Common PHY-BF']"/>
      <xi:include href="PHY.xml" xpath="//definitions[@name='PHY-TX']"/>
