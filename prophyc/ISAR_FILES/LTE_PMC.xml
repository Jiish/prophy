            <message name="PHY_Start3GppDlTestModelReq"/>
            <message name="PHY_Start3GppDlTestModelResp"/>
            <message name="PHY_Stop3GppDlTestModelReq"/>
            <message name="PHY_Stop3GppDlTestModelResp"/>
         </interface>
      </interfaces>
